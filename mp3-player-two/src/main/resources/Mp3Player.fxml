<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<?import customjavafx.scene.control.YieldingSlider?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guilgaly.fxtest.mp3player.Mp3PlayerController">
	<stylesheets>
		<URL value="@Mp3Player.css" />
	</stylesheets>
   <top>
      <MenuBar id="menuBar" fx:id="menuBar" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#onOpen" text="Open" />
                  <MenuItem mnemonicParsing="false" onAction="#onClose" text="Close" />
                  <SeparatorMenuItem mnemonicParsing="false" />
              <MenuItem mnemonicParsing="false" onAction="#onQuit" text="Quit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <ScrollPane fitToHeight="true" fitToWidth="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <content>
                <AnchorPane>
                     <children>
                        <TableView id="metadataTable" fx:id="metadataTable" focusTraversable="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                  </AnchorPane>
              </content>
            </ScrollPane>
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints fillWidth="false" />
            <ColumnConstraints fillWidth="false" />
            <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
            <ColumnConstraints fillWidth="false" />
            <ColumnConstraints fillWidth="false" />
            <ColumnConstraints fillWidth="false" />
            <ColumnConstraints fillWidth="false" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints />
            <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button id="startStopButton" fx:id="startStopButton" mnemonicParsing="false" onAction="#onStartStop" prefWidth="60.0" text="Play" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin>
            </Button>
            <Label id="timeDisplay" fx:id="timeDisplay" text="00:00" GridPane.columnIndex="3" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin></Label>
            <Slider id="volumeSlider" fx:id="volumeSlider" blockIncrement="0.1" max="1.0" prefWidth="100.0" GridPane.columnIndex="5" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets right="5.0" />
               </GridPane.margin></Slider>
            <Label text="-" GridPane.columnIndex="4" GridPane.rowIndex="1">
               <padding>
                  <Insets right="5.0" />
               </padding></Label>
            <Label text="+" GridPane.columnIndex="6" GridPane.rowIndex="1" />
            <YieldingSlider id="seekSlider" fx:id="seekSlider" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin>
            </YieldingSlider>
            <Button id="closeButton" mnemonicParsing="false" onAction="#onClose" prefWidth="60.0" text="Stop" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
   </bottom>
</BorderPane>
